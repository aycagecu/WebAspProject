@{
    var bsvr = ViewBag.basvuru;
}


@foreach(var b in bsvr){
    
   <div>
        <h4>@b.u.UserAd</h4>
        <h4>@b.u.UserSoyad</h4>
        <h4>@b.u.UserName</h4>
        <div>
            <vc:hayvan-vc h=@b.h></vc:hayvan-vc>
        </div>
        <h4>@b.b.BasvuruFormu</h4>
        <h4>@b.b.BasvuruTarihi</h4>
        <div>
            <a asp-action="Onayla" asp-controller="Admin" asp-route-id="@b.b.BasvuruId" class="button">Onayla</a>
            <a asp-action="Reddet" asp-controller="Admin" asp-route-id="@b.b.BasvuruId" class ="button">Reddet</a>
        </div>
   </div>
}